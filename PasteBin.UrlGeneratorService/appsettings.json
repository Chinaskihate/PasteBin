{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "UrlGeneratorServiceSettings": {
        "DbSettings": {
            "ConnectionString": "{db_connection}"
        },
        "Redis": {
            "ConnectionString": "{redis_connection}"
        },
        "UrlSetName": "short-urls",
        "MinUrlCount": 10,
        "DelayOnStop": "00:00:10",
        "BatchSize": 2
    }
}
